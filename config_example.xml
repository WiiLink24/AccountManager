<Config>
    <!-- The address which HTTP will bind to -->
    <address>127.0.0.1:9011</address>

    <!-- OIDC credentials/data -->
    <oidc>
        <clientID></clientID>
        <clientSecret></clientSecret>
        <redirectURL>http://localhost:9011/authorize</redirectURL>
        <scopes>openid profile email goauthentik.io/api wii offline_access</scopes>
        <provider></provider>
        <logoutURL></logoutURL>
        <serviceAccountToken></serviceAccountToken>
    </oidc>
</Config>